#include<bits/stdc++.h>
using namespace std;
int main()
{
 long long n;
 cin>>n;
 long long arr[n];
 for(int i=0;i<n;i++)
 {
     cin>>arr[i];
 }
 bool flag=true;
 long long i=0;
 long long j=n-1;
 while(i<=j)
 {
   if (arr[i]!=arr[j])
   {
       flag=false;
       break;
   }
   i++;
   j--;
}
 if(flag==true)
 {
     cout<<"YES"<<endl;
 }
 else
 {
     cout<<"NO"<<endl;
 }
}
